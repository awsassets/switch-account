{
   "background": {
      "scripts": [
         "background.js"
      ]
   },
   "browser_action": {
      "default_icon": "switch.png",
      "default_popup": "popup.html",
      "default_title": ""
   },
   "content_scripts": [
      {
         "js": [
            "contentscript.js"
         ],
         "matches": [
            "*://*/*"
         ]
      }
   ],
   "description": "快速切换同一网站的多个已登录账号，支持豆瓣、知乎、微博、Twitter、Facebook 等等",
   "icons": {
      "128": "switch.png",
      "16": "switch.png",
      "48": "switch.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "站点多账户管理",
   "permissions": [
      "cookies",
      "tabs",
      "http://*/*",
      "https://*/*",
      "storage"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}